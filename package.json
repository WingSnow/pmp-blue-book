{
  "name": "pmp-blue-book",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "vite": "vite",
    "watch-serve": "nodemon --watch ./server/src -e ts,tsx --exec npx ts-node ./server/src/app.ts",
    "dev": "concurrently \"npm run watch-serve\" \"npm run vite\"",
    "build": "vue-tsc --noEmit && vite build && cd server && tsc",
    "start": "node ./dist/app.js"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.3",
    "concurrently": "^7.2.1",
    "dayjs": "^1.11.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.3",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa2-history-api-fallback": "^0.1.0",
    "log4js": "^6.5.2",
    "sqlite3": "^5.0.8",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-log4": "^2.3.3",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^17.0.33",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-visualizer": "^5.6.0",
